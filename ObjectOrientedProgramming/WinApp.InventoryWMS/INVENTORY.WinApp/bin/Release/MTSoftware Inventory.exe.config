<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="MTIMS.Data" type="MTIMS.Data.Bases.NetTiersServiceSection, MTIMS.Data" allowDefinition="MachineToApplication" restartOnExternalChanges="true" />
  </configSections>
  <connectionStrings>
    <add name="MTIMSConnectionString" connectionString="Data Source=.;Database=MT.INVENTORY;UID=sa;PWD=sa" />
  </connectionStrings>
  <MTIMS.Data defaultProvider="SqlNetTiersProvider">
    <providers>
      <add name="SqlNetTiersProvider" type="MTIMS.Data.SqlClient.SqlNetTiersProvider, MTIMS.Data.SqlClient" connectionStringName="MTIMSConnectionString" providerInvariantName="System.Data.SqlClient" entityFactoryType="MTIMS.Entities.EntityFactory" useEntityFactory="true" enableEntityTracking="false" enableMethodAuthorization="false" useStoredProcedure="true" />
    </providers>
  </MTIMS.Data>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
</configuration>
